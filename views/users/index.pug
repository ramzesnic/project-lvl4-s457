extends /layouts/application.pug

block content
  table.table.table-stripped.table-hover.mt-4
    thead
      tr
        th= t('view.user.full_name')
        th= t('view.user.email')
        th= t('view.user.created_at')
    tbody
      each user in users
        tr
          td= user.fullName
          td= user.email
          td= user.formatedDate